 .syntax unified
 .cpu cortex-m4
 .thumb

.data
	result: .word 0
	max_size: .word 0
.text
	m: .word 0x5E
	n: .word 0x60

main:
 // r0 = m, r1 = n
 ldr r1, =m
 ldr r2, =n
 push {r1,r2,lr}
 movs r3, #1 // r10 = ans
 BL GCD
 // get return val and store into result

 GCD:
 //TODO: Implement your GCD function
 //pop {r1,r2}
 ldr r1, [sp]
 ldr r2, [sp, #4]
 ldr r3, [sp, #8]
 cmp r1, #0

 BX LR
